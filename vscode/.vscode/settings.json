{
  "git.enabled": false,
  "git.ignoredRepositories": ["."],
  "files.exclude": {
    "**/node_modules": true,
    "**/.git": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".cache": true,
    ".local": true,
    ".Trash": true,
    ".vscode/extensions": true,
    "Applications": true,
    "Desktop": true,
    "Documents": true,
    "Downloads": true,
    "Dropbox": true,
    "Library": true,
    "Movies": true,
    "Music": true,
    "Pictures": true,
    "Public": true,
    "go": true,
    "sdk": true,
    "data": true,
    "configs": true,
    "log": true,
    "iboysoft-extensions": true,
    "Virtual Machines.localized": true,
    "Sites": true,
    "Qu": true,
    "OrbStack": true
  },
  "files.associations": {
    "*.zshrc": "shellscript",
    "*.bashrc": "shellscript", 
    "*.bash_profile": "shellscript",
    "*.zprofile": "shellscript",
    ".p10k.zsh": "shellscript",
    "*.env": "dotenv",
    "*.env.*": "dotenv",
    ".gitconfig": "gitconfig",
    ".gitignore*": "gitignore"
  },
  "shellcheck.enable": true,
  "dotfiles.repository": "~/.dotfiles"
}
